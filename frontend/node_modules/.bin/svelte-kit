#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules/@sveltejs/kit/node_modules:/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules/@sveltejs/node_modules:/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules:/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules/@sveltejs/kit/node_modules:/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules/@sveltejs/node_modules:/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules:/mnt/c/Users/keyme/proyectos/moda-organica/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
else
  exec node  "$basedir/../.pnpm/@sveltejs+kit@2.47.2_@svelt_2e804a0a27b2d423f89e27d4b87ef6b6/node_modules/@sveltejs/kit/svelte-kit.js" "$@"
fi
